{
      "manifest_version": 3,
      "name": "Custom CSS for Specific Page",
      "version": "1.0",
      "description": "Change CSS of a specific webpage",
      "permissions": ["activeTab", "scripting"],
      "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "css": ["styles.css"],
          "js": ["content.js"]
        }
      ]
    }